#iclude <iostream>
#include <vector>
using namespace std;

int main() {
  int n;
  cin >> n;
  
  //запись вектора с клавиатуры;
  vector <int> a(n);
  for (int i = 0; i < n; i++){
      cin >> a[i];
  }
  
  //разворот вектора и вывод;
  
  for(int i = n - 1; i >= 0; i--){
    cout << a[i] << " ";}
  

return 0;
}
