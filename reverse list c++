#include <iostream>

using namespace std;

int main() {
    int x;
    cin >> x;

    int* list = new int[x];

    for (int i = 0; i < x; i++) {
        list[i] = 0;
    }

    for (int i = x - 1; i >= 0; i--) {
        cin >> list[i];
    }




    for (int i = 0; i < x; i++) {
        cout << list[i] << " ";
    }

    return 0;
}
